{
  "name": "@phsd/lint",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    "./eslint": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "pnpm dev",
    "dev:inspector": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "build": "tsdown",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "build:types": "pnpm tsx scripts/typegen.ts",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "check:test": "vitest run",
    "check:types": "tsc --noEmit",
    "check:types:watch": "tsc --noEmit --watch",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "prepublish": "publint"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "catalog:lint",
    "@eslint/js": "catalog:lint",
    "@eslint/markdown": "catalog:lint",
    "@stylistic/eslint-plugin": "catalog:lint",
    "@typescript-eslint/eslint-plugin": "catalog:types",
    "@typescript-eslint/parser": "catalog:types",
    "@vitest/eslint-plugin": "catalog:lint",
    "astro-eslint-parser": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-config-flat-gitignore": "catalog:lint",
    "eslint-config-turbo": "catalog:monorepo",
    "eslint-flat-config-utils": "catalog:lint",
    "eslint-merge-processors": "catalog:lint",
    "eslint-plugin-antfu": "catalog:lint",
    "eslint-plugin-astro": "catalog:lint",
    "eslint-plugin-better-tailwindcss": "catalog:lint",
    "eslint-plugin-command": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "eslint-plugin-import-lite": "catalog:lint",
    "eslint-plugin-jsdoc": "catalog:lint",
    "eslint-plugin-jsonc": "catalog:lint",
    "eslint-plugin-n": "catalog:lint",
    "eslint-plugin-perfectionist": "catalog:lint",
    "eslint-plugin-pnpm": "catalog:lint",
    "eslint-plugin-react-hooks": "catalog:lint",
    "eslint-plugin-react-refresh": "catalog:lint",
    "eslint-plugin-regexp": "catalog:lint",
    "eslint-plugin-svelte": "catalog:lint",
    "eslint-plugin-toml": "catalog:lint",
    "eslint-plugin-unicorn": "catalog:lint",
    "eslint-plugin-unused-imports": "catalog:lint",
    "eslint-plugin-yml": "catalog:lint",
    "eslint-typegen": "catalog:lint",
    "globals": "catalog:node",
    "jsonc-eslint-parser": "catalog:lint",
    "prettier-plugin-astro": "catalog:lint",
    "prettier-plugin-svelte": "catalog:lint",
    "publint": "catalog:lint",
    "svelte-eslint-parser": "catalog:lint",
    "toml-eslint-parser": "catalog:lint",
    "typescript-eslint": "catalog:lint",
    "yaml-eslint-parser": "catalog:lint"
  },
  "devDependencies": {
    "@phsd/format": "workspace:*",
    "@phsd/typescript": "workspace:*",
    "@types/node": "catalog:types",
    "local-pkg": "catalog:node",
    "prettier": "catalog:lint",
    "tailwindcss": "catalog:style",
    "tsdown": "catalog:build",
    "tsx": "catalog:script",
    "typescript": "catalog:build"
  }
}
